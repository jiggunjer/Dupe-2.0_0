{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "duplicateTab_19.png",
      "default_title": "Dupe Shortcut Key"
   },
   "commands": {
      "duplicate": {
         "description": "Duplicate Tab",
         "suggested_key": {
            "default": "Ctrl+E"
         }
      },
      "duplicateToWindow": {
         "description": "Duplicate Tab into New Window",
         "suggested_key": {
            "default": "Ctrl+Y"
         }
      }
   },
   "content_security_policy": "script-src 'self' https://ajax.googleapis.com; script-src 'self' https://ajax.googleapis.com; object-src 'self'",
   "description": "Adds a shortcut key to allow duplicating a tab.",
   "icons": {
      "128": "duplicateTab_128.png",
      "16": "duplicateTab_16.png",
      "48": "duplicateTab_48.png"
   },   
   "manifest_version": 2,
   "minimum_chrome_version": "25",
   "name": "Dupe",
   "options_page": "options.html",
   "permissions": [ "tabs", "webRequest", "webRequestBlocking", "*://*/*" ],   
   "version": "2.0"
}
